class demo
{
    public static void main(String args[])
    {
        int a[]={33,3,4,5};
        for(int i=0;i<a.length;i++)
        System.out.println(a[i]);
        
    }
}


class demo
{
    void display()
    {
        System.out.println("hello");
    }
    public static void main(String args[])
    {
        demo d= new demo();
        d.display();
    }
}

with parameters:
class demo
{
    void display(int a, int b)
    {
        int sum=0;
        sum=sum+a+b;
        System.out.println(sum);
    }
    public static void main(String args[])
    {
        demo d= new demo();
        d.display(8,2);
    }
}


constructor

class bike
{
    bike()
    {
        System.out.println("bike is created");
    }
    public static void main(String args[])
    {
        bike b= new bike();
    }
}


class display
{
    int id;
    String name;
    display(int i,String names)
    {
        id=i;
        name=names;
    }
    void display1()
    {
        System.out.println("name ="+name);
        System.out.print("id="+id);
    
    }
    public static void main(String[]args)
    {
        display d=new display(422,"arun");
        d.display1();
    }
}


class students
{
    int id;
    String name;
    int age;
    students (int i,String n)
    { id =i;
    name = n;
    }
    students(int i,String n,int a)
    {
        id=i;
        name =n;
        age=a;
    }
    void display()
    {
        System.out.println(id+""+name+""+age);
    }
    public static void main(String  args[])
    {
        students s1=new
        students(111," karan ",24);
        students s2=new
        students(222," aryan ",25);
        s1.display();
        s2.display();
    }
}


STRING

class demo
{
    public static void main(String args[])
    {
        String s=new String("java");
        System.out.println(s);
    }
}


LENGTH
import java.lang.*;
class demo
{
    public static void main(String args[])
    {
       int l;
       String s=new String("java");
       l=s.length();
       System.out.println("length:"+l);
    }
}



chatAt(index)

import java.lang.*;
class demo
{
    public static void main(String args[])
    {
       char c;
       String s=new String("entertainment");
       c=s.charAt(7);
       System.out.println("character:"+c);
    }
}



COMBINED STRING
class demo
{
    public static void main(String args[])
    {
        String s1="aryan";
        String s2="kumar";
        System.out.println("combined string:"+s1.concat(s2));
    }
}


EQUALS

class demo
{
    public static void main(String args[])
    {
        String s1="aryan";
        String s2="kumar";
        System.out.println("combined string:"+s1.equals(s2));
    }
}


COMPARE STRING

class demo
{
    public static void main(String args[])
    {
        String s1="aryan";
        String s2="ARYAN";
        System.out.println("compare string:"+s1.equalsIgnoreCase(s2));
    }
}


sub string()

class demo
{
    public static void main(String args[])
    {
        String s="java is a programing language";
        System.out.println(s.substring(6));
    }
}

STRING BUFFER 

class demo
{
    public static void main(String args[])
    {
        StringBuffer sb= new StringBuffer("java");
        sb.append("software");
        System.out.println(sb);
    }
}

STRING BUFFER REVERSE

class demo
{
    public static void main(String args[])
    {
        StringBuffer sb= new StringBuffer("saveetha engineering college");
        System.out.println(sb.reverse());
    }
}

INSERT()

class demo
{
    public static void main(String args[])
    {
        StringBuffer sb= new StringBuffer("saveetha engineering college");
        System.out.println(sb.insert(28,"first"));
    }
}






